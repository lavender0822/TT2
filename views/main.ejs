<body>
  <label for="import‑file">Upload the testfile:</label>
  <input type="file" name="import‑file" id="import‑file" accept="application/json" required>
  <button onclick="getActionData()">Submit</button>
  
  <p>
      <span id="json‑file"></span>
  </p>

